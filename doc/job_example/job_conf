[cocci]
# The name is important. You should avoid using same name for different
# semantic patches. Notice that the example url ends with: ${name}
# You should try split the url this way when possible. For the semantic
# patch you can use url or file. curl will download the url. The config
# parser expect at least one leading space for each line of the cocci
# file you paste here.

name: e100_probe.cocci
url: https://raw.githubusercontent.com/petersenna/cloudspatch/master/doc/${name}
#file: @@
# @@
# * e100_probe(...)
# {...}

